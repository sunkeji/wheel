<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="albumback">#E1E0DE</color>
    <color name="bar_color">#f4f4f4</color>
    <color name="black">#000</color>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <color name="cp_alpha_gray">#CC646464</color>
    <color name="cp_colorAccent">#F3F3F3</color>
    <color name="cp_colorPrimary">@color/cp_colorAccent</color>
    <color name="cp_colorPrimaryDark">@color/cp_colorAccent</color>
    <color name="cp_common_bg">#FAFAFA</color>
    <color name="cp_content_bg">@color/cp_colorAccent</color>
    <color name="cp_divider">#ddd</color>
    <color name="cp_gray">#999</color>
    <color name="cp_gray_deep">#333</color>
    <color name="cp_shadow">#AAEBEBEB</color>
    <color name="cp_theme_gray">#646464</color>
    <color name="hot_bg">#eee</color>
    <color name="line_color">#ddd</color>
    <color name="main_color">#ea5513</color>
    <color name="no_color">#00000000</color>
    <color name="recycler_swipe_color_loading_color1">#55777777</color>
    <color name="recycler_swipe_color_loading_color2">#B1777777</color>
    <color name="recycler_swipe_color_loading_color3">#FF777777</color>
    <color name="recycler_swipe_color_text_gray">#FF777777</color>
    <color name="transparent">#00000000</color>
    <color name="white">#ffffff</color>
    <declare-styleable name="gridPasswordView"><attr format="color|reference" name="gpvTextColor"/><attr format="dimension" name="gpvTextSize"/><attr format="color" name="gpvLineColor"/><attr format="color" name="gpvGridColor"/><attr format="dimension" name="gpvLineWidth"/><attr format="integer" name="gpvGridCornerRadius"/><attr format="integer" name="gpvPasswordLength"/><attr format="string" name="gpvPasswordTransformation"/><attr format="enum" name="gpvPasswordType">
            <enum name="numberPassword" value="0"/>
            <enum name="textPassword" value="1"/>
            <enum name="textVisiblePassword" value="2"/>
            <enum name="textWebPassword" value="3"/>
        </attr></declare-styleable>
    <declare-styleable name="recycler_swipe_SwipeMenuLayout"><attr format="reference" name="leftViewId"/><attr format="reference" name="rightViewId"/><attr format="reference" name="contentViewId"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="city_text_size">16sp</dimen>
    <dimen name="side_letter_bar_letter_size">12sp</dimen>
    <dimen name="side_letter_bar_width">35dp</dimen>
    <string name="app_name">wheel</string>
    <string name="back">返回</string>
    <string name="cp_hint_search_box">请输入城市名或拼音</string>
    <string name="cp_hot_city">热门城市</string>
    <string name="cp_located_city">当前位置</string>
    <string name="cp_located_failed">定位失败</string>
    <string name="cp_locating">正在定位…</string>
    <string name="cp_nothing_found">没有搜索结果~</string>
    <string name="cp_select_city">选择城市</string>
    <string name="no_photo">相册没有图片</string>
    <string name="only_choose_num">超出可选图片张数</string>
    <string name="preview">预览</string>
    <string name="recycler_swipe_click_load_more">Click to load more</string>
    <string name="recycler_swipe_data_empty">There is no data</string>
    <string name="recycler_swipe_load_error">Error, please try again</string>
    <string name="recycler_swipe_load_more_message">Loading, please wait later</string>
    <string name="recycler_swipe_more_not">No more data</string>
    <string name="unfinished">完成</string>
    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="GridPasswordView"/>
    <style name="GridPasswordView.Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#dfdfdf</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
    </style>
    <style name="GridPasswordView.EditText" parent="GridPasswordView.TextView">
        <item name="android:cursorVisible">false</item>
    </style>
    <style name="GridPasswordView.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">#535353</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="LetterIndexTextViewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">@dimen/side_letter_bar_width</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/cp_gray</item>
    </style>
    <style name="line_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">#DCDCDC</item>
        <item name="android:gravity">center</item>
    </style>
</resources>